<Defs>
  
    <!-- ====================== Plasma Gun  ===================== -->

    <ThingDef ParentName="BaseBullet">
        <defName>VFEP_Bullet_PlasmaShot</defName>
        <label>plasma shot</label>
        <graphicData>
            <texPath>Things/Projectiles/Plasma_Projectile</texPath>
