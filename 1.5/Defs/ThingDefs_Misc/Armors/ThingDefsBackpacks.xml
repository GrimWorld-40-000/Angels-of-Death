<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!--Generic armor base. Every armor will have these things. Only add/change stuff in this if you want every armor to be affected-->
	<ThingDef Abstract="true" Name="GW_SM_BackpackBase" ParentName="GW_BackpackBase">
		<techLevel>Spacer</techLevel>
		<description>Astartes backpacks serve as power hubs for their armors, providing energy to various systems crucial for combat. These backpacks house fusion reactors and energy storage units, supplying the necessary power for mobility, communication, and weapons systems. Designed for reliability and efficiency, they ensure that Space Marines remain fully operational on the battlefield, powering their armor's functions and enhancing their combat effectiveness.</description>
		<uiIconScale>1.6</uiIconScale>
		<recipeMaker>
			<unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
			<skillRequirements>
				<Crafting>15</Crafting>
			</skillRequirements>
			<recipeUsers Inherit="false">
				<li>GW_SM_ArmorBench</li>
			</recipeUsers>
		</recipeMaker>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>1</drawSize>
		</graphicData>
		<tradeTags>
			<li>GW_SM_Backpacks</li>
		</tradeTags>
		<apparel>
			<tags>
				<li>Spacer</li>
			</tags>
		</apparel>
		<equippedStatOffsets>
            <CarryBulk MayRequire="CETeam.CombatExtended">50</CarryBulk>
		</equippedStatOffsets>
		<comps>
			<li Class="GW4KArmor.CompProperties_TriColorMask">
				<maskCount>2</maskCount>
				<palettePresets>GW_SM_BackpackChapterPalettes</palettePresets>
				<defaultPalette>
					<colorA>(250, 176, 67)</colorA>
					<colorB>(250, 176, 67)</colorB>
					<colorC>(180, 65, 74)</colorC>
				</defaultPalette>
			</li>
		</comps>
	</ThingDef>

	<!--MKX Reiver-->
	<ThingDef ParentName="GW_SM_BackpackBase">
		<defName>GW_SM_ReiverBackpack</defName>
		<label>reiver backpack</label>
		<graphicData>
			<texPath>Things/Apparel/Armor_Reiver/Reiver_backpack_south</texPath>
		</graphicData>
		<recipeMaker>
			<researchPrerequisite>GW_SM_AstartesLight</researchPrerequisite>
		</recipeMaker>
		<statBases>
			<WorkToMake>2500</WorkToMake>
			<MaxHitPoints>500</MaxHitPoints>
			<Mass>2</Mass>
			<Flammability>0.2</Flammability>
			<ArmorRating_Sharp>0.65</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.45</ArmorRating_Blunt>
			<ArmorRating_Heat>0.5</ArmorRating_Heat>
			<Insulation_Cold>10</Insulation_Cold>
			<Insulation_Heat>10</Insulation_Heat>
			<EquipDelay>2</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>0.8</MoveSpeed>
			<VEF_MassCarryCapacity>50</VEF_MassCarryCapacity>
		</equippedStatOffsets>
		<costList>
			<ComponentSpacer>3</ComponentSpacer>
			<Plasteel>50</Plasteel>
			<Steel>70</Steel>
			<Uranium>10</Uranium>
			<HP_Ceramite>5</HP_Ceramite>
		</costList>
		<apparel>
			<wornGraphicPath>Things/Apparel/Armor_Reiver/Reiver_backpack</wornGraphicPath>
			<tags>
				<li>GW_Reiver</li>
			</tags>
		</apparel>
	</ThingDef>

