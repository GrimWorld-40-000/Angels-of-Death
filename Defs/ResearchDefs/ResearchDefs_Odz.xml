<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <ResearchProjectDef Abstract="true" Name="HP_ResearchProject_Base">
		<techLevel>Ultra</techLevel>
		<prerequisites>
			<li>HP_ImperialFabricationBench</li>
		</prerequisites>
		<requiredResearchFacilities>
			<li>MultiAnalyzer</li>
		</requiredResearchFacilities>
    </ResearchProjectDef>

            <!--To add a new category, copy one of the ones below, and change the tab and baseCost to the desired amount. If one of them requires completely different prereqs, either remove the parent name(remember to add everything in the normal base to the new one), or remove the prerequisite from the base to add to all of these bases-->

    <ResearchProjectDef Abstract="true" Name="HP_Armors_AA_Base" ParentName="HP_ResearchProject_Base">
		<tab>Armors_AA</tab>
		<baseCost>4000</baseCost>
    </ResearchProjectDef>

    <ResearchProjectDef Abstract="true" Name="HP_Adeptus_Base" ParentName="HP_ResearchProject_Base">
		<tab>Odz_ResearchTab_Adeptus</tab>
		<baseCost>12000</baseCost>
    </ResearchProjectDef>

            <!--This area is for adding new prerequisite researches, like the basic fabrication bench research. If there are other researches that act primarily as prerequisites, add them here-->

    <ResearchProjectDef>
        <defName>HP_ImperialFabricationBench</defName>
		<label>Astartes Fabrication Bench</label>
		<description>Its workstation which allow you to craft Adeptus Astartes armors and weapons from Warhammer 40,000 universe</description>
		<tab>Odz_ResearchTab_Adeptus</tab>
		<techLevel>Ultra</techLevel>
		<requiredResearchFacilities>
			<li>MultiAnalyzer</li>
		</requiredResearchFacilities>
		<baseCost>8000</baseCost>
		<researchViewX>0.00</researchViewX>
		<researchViewY>2.00</researchViewY>
	</ResearchProjectDef>

