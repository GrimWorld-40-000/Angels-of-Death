<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <ApparelLayerDef>
        <defName>GW_Shoulder</defName>
        <label>shoulder</label>
        <drawOrder>550</drawOrder>
    </ApparelLayerDef>

    <ThingCategoryDef>
        <defName>GW_ArmorShoulderPad</defName>
        <label>shoulder pads</label>
        <parent>ApparelArmor</parent>
    </ThingCategoryDef>

        <!--Generic shoulder base. Every armor will have these things. Only add/change stuff in this if you want every armor to be affected-->
    <ThingDef Abstract="true" Name="HP_AoD_ShoulderBase" ParentName="ArmorMachineableBase">
        <techLevel>Spacer</techLevel>
		<recipeMaker>
			<unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
			<skillRequirements>
				<Crafting>15</Crafting>
			</skillRequirements>
			<recipeUsers Inherit="false">
				<li>HP_ImperialFabricationBench</li>
			</recipeUsers>
		</recipeMaker>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>1</drawSize>
		</graphicData>
		<thingCategories>
			<li>GW_ArmorShoulderPad</li>
		</thingCategories>
        <tradeTags>
            <li>HPArmor</li>
        </tradeTags>
		<apparel>
			<countsAsClothingForNudity>false</countsAsClothingForNudity>
			<bodyPartGroups>
				<li>Neck</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li MayRequire="ceteam.combatextended">Hands</li>
			</bodyPartGroups>
			<layers>
				<li>GW_Shoulder</li>
			</layers>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
		</apparel>
	    <modExtensions>
            <li Class="CombatExtended.PartialArmorExt" MayRequire="ceteam.combatextended">
                <stats>		  				  
                    <li>
                        <ArmorRating_Sharp>0.8</ArmorRating_Sharp>
                        <parts>
                            <li>Hand</li>
                        </parts>
                    </li>
                    <li>
                        <ArmorRating_Blunt>0.8</ArmorRating_Blunt>
                        <parts>
                            <li>Hand</li>
                        </parts>
                    </li>
                </stats>
            </li>
			<li Class="VFECore.ApparelDrawPosExtension">
				<apparelDrawSettings>
                    <drawPosNorthOffset>(0, 0.3, 0)</drawPosNorthOffset>
                    <drawPosSouthOffset>(0, 0, 0)</drawPosSouthOffset>
                    <drawPosWestOffset>(0, 0.3, 0)</drawPosWestOffset>
                    <drawPosEastOffset>(0, 0.3, 0)</drawPosEastOffset>
                </apparelDrawSettings>
                <isUnifiedApparel>true</isUnifiedApparel>
				<hideHead>false</hideHead>
            </li>
	    </modExtensions>
    </ThingDef>

		<!--MKX Reiver-->

	<ThingDef Abstract="True" Name="GW_SM_MKXReiverShoulderPads" ParentName="HP_AoD_ShoulderBase">
		<description>Power armour is an advanced form of powered combat armour, worn primarily by the Space Marines and the Chaos Space Marines, though suits have been created to be worn by mere mortals. It is a completely enclosed suit of combat armour composed of shaped adamantium and plasteel plates, encased in a ceramite ablative layer. This is the Reiver version.</description>
		<researchPrerequisites>
			<li>GW_SM_AstartesLight</li>
		</researchPrerequisites>
		<statBases>
			<WorkToMake>5000</WorkToMake>
			<MaxHitPoints>500</MaxHitPoints>
			<Mass>3</Mass>
			<Flammability>0.2</Flammability>
			<ArmorRating_Sharp>1.30</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.90</ArmorRating_Blunt>
			<ArmorRating_Heat>1.0</ArmorRating_Heat>
			<Insulation_Cold>20</Insulation_Cold>
			<Insulation_Heat>20</Insulation_Heat>
			<EquipDelay>2</EquipDelay>
		</statBases>
		<costList>
			<ComponentSpacer>3</ComponentSpacer>
			<Plasteel>150</Plasteel>
			<Steel>130</Steel>
			<Uranium>75</Uranium>
			<HP_Ceramite>25</HP_Ceramite>
		</costList>
		<apparel>
			<tags>
				<li>GW_Reiver</li>
			</tags>
		</apparel>
	</ThingDef>

