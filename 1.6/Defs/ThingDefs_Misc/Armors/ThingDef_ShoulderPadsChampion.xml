	<!-- Dark Angel -->
	
	<ThingDef Name="GW_CM_DarkAngelShoulder_Base" ParentName="HP_AoD_ShoulderBase">
		<defName>GW_SM_DarkAngelsShoulderPadsModelA_DA</defName>
		<label>Dark Angel shoulder pads (a)</label>
		<description>The shoulder pads of a Champion of the Dark Angels. \n\nThe Dark Angels do not have a homeworld, since Caliban was destroyed. Instead, they operate out of "The Rock" - a floating fortress-monastery that was once on the surface of Caliban.</description>
		<graphicData>
			<texPath>Things/Apparel/DarkAngel/Shoulders/DAGravis_sp_modelA_south</texPath>
		</graphicData>
		<apparel>
			<wornGraphicPath>Things/Apparel/DarkAngel/Shoulders/DAGravis_sp_modelA</wornGraphicPath>
		</apparel>
		<statBases>
			<WorkToMake>25000</WorkToMake>
			<MaxHitPoints>260</MaxHitPoints>
			<Mass>2.0</Mass>
			<MarketValue>400</MarketValue>
			<Flammability>0.2</Flammability>
			<ArmorRating_Sharp>0.35</ArmorRating_Sharp>
			<ArmorRating_Blunt>0.20</ArmorRating_Blunt>
			<ArmorRating_Heat>0.22</ArmorRating_Heat>
			<Insulation_Cold>3</Insulation_Cold>
			<Insulation_Heat>5</Insulation_Heat>
			<EquipDelay>2</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<ToxicEnvironmentResistance>0.25</ToxicEnvironmentResistance>
			<MoveSpeed>-0.1</MoveSpeed>
		</equippedStatOffsets>
		<costList>
			<Plasteel>90</Plasteel>
			<Steel>150</Steel>
			<HP_Ceramite>100</HP_Ceramite>
		</costList>
